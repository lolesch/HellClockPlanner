      manaCost: 300
      cooldown: 1
      baseDamage: 0
    - name: DoubleKnives
      id: 69
      description: Continuously spin two blades around while you move, dealing Physical
        damage to nearby enemies.
      manaCost: 34
      cooldown: 0.5
      baseDamage: 80
